{
  "Module": {
    "Name": "NewsVendor",
    "Type": "stochastic",
    "Author": "Alan King",
    "Date": "1 March 2019"
  },
  "Stages": [
    "before_demand",
    "after_demand"
  ],
  "Index": {
    "newspaper": {
      "Ordered": false
    }
  },
  "Param": {
    "budget": {
      "Type": "Double",
      "Sense": "geq",
      "Stage": "before_demand"
    },
    "cost": {
      "Type": "Double",
      "Index": "newspaper",
      "Sense": "geq",
      "Stage": "before_demand"
    },
    "price": {
      "Type": "Double",
      "Index": "newspaper",
      "Sense": "geq",
      "Stage": "after_demand"
    },
    "demand": {
      "Type": "Double",
      "Index": "newspaper",
      "Sense": "geq",
      "Stage": "after_demand"
    }
  },
  "Var": {
    "order": {
      "Type": "Integer",
      "Index": "newspaper",
      "Sense": "geq",
      "Stage": "before_demand"
    },
    "sold": {
      "Type": "Integer",
      "Sense": "geq",
      "Stage": "after_demand"
    }
  },
  "Expression": {
    "budget": {
      "Sense": "geq",
      "Value": "Param.budget - Var.order * Param.cost",
      "Stage": "before_demand"
    },
    "sold_less_than_ordered": {
      "Index": "newspaper",
      "Sense": "geq",
      "Value": "Var.order - Var.sold",
      "Stage": "after_demand"
    },
    "sold_less_than_demanded": {
      "Index": "newspaper",
      "Sense": "geq",
      "Value": "Param.demand - Var.sold",
      "Stage": "after_demand"
    },
    "revenue": {
      "Sense": "free",
      "Value": "Param.price * Var.sold - Param.cost * Var.ordered",
      "Stage": "after_demand"
    }
  },
  "Optimize": {
    "Value": "Expression.revenue",
    "Sense": "maximize"
  }
}
